{
    "name": "oselya/ip-geolocation-bundle",
    "description": "Symfony IP geolocation bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oselya\\IpGeolocationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oselya\\IpGeolocationBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=8.3",
        "geoip2/geoip2": "^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/cache": "^7.1",
        "symfony/config": "^7.1",
        "symfony/console": "^7.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/deprecation-contracts": "^3.5",
        "symfony/http-kernel": "^7.1",
        "symfony/intl": "^7.1",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/phpunit-bridge": "^7.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
